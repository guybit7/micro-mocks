<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    
   	<changeSet id="202010211819" author="GuyBiton">
		<preConditions onFail="MARK_RAN">
			<not>
				<tableExists tableName="user" schemaName=""/>
			</not>
			<not>
				<tableExists tableName="variant" schemaName=""/>
			</not>
		</preConditions>
		<createTable tableName="mock_item">
			<column name="id" type="bigint">
				<constraints primaryKey="true" primaryKeyName="mock_item_id_pk" />
			</column>
			<column name="endpoint" type="varchar(250)">
				<constraints nullable="false"/>
			</column>
			<column name="value" type="JSON">
				<constraints nullable="false"/>
			</column>
			<column name="user_id" type="int"/>
			<column name="variant_id" type="int"/>
		</createTable>
	</changeSet>
	
</databaseChangeLog>